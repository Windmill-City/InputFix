<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:STALauncher"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <sys:String x:Key="L_INSTALL_STA">
        Install STALauncher
    </sys:String>
    <sys:String x:Key="L_INSTALL_SMAPI">
        Install SMAPI
    </sys:String>
    <sys:String x:Key="L_INSTALL_STA_Desc">
        Select your game path, will copy the lanucher to the path
        *Notice: the path where Stardew Valley.exe inside
    </sys:String>
    <sys:String x:Key="L_INSTALL_STA_Desc_NotGamePath">
        This seems not a vaild game path, force copy?
    </sys:String>
    <sys:String x:Key="L_INSTALL_STA_Desc_Done">
        STALanncher has copied to the path
    </sys:String>
    <sys:String x:Key="L_INSTALL_STA_Desc_Fail">
        Fail to copy the file to the path, maybe you need to copy manually
    </sys:String>
    <sys:String x:Key="L_INSTALL_SMAPI_Desc">
        Download from smapi.io, click to open the link
    </sys:String>
</ResourceDictionary>